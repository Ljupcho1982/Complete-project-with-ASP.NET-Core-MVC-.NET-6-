@model IEnumerable<Category>
@*
   ViewData["Title"]="Index";

  
*@
@{
}

<h1>Index Category</h1>


<table class="table-bordered table-striped" style="width:100%">
    <thead>
        <tr>

            <th>

                Category Name
            </th>
            <th>
                Display Order
            </th>
            <th>

                Time

            </th>
        </tr>

    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>
                    @obj.Name

                </td>
                

            
            
           <td>
                    @obj.DisplayOrde

           </td>
                
            <td>
                    @obj.CreatedDateTime
            </td>
            
            
           

        </tr>
        }




    </tbody>




</table>